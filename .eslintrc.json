{
    "env": {
        "browser": true,
        "es2020": true,
        "jquery": true
    },
    "globals": {
        "ColorThief": "readonly"
    },
    "extends": "eslint:all",
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
        "array-element-newline": ["error", {
            "minItems": 4
        }],
        "capitalized-comments": "off",
        "dot-location": "off",
        "eol-last": "off",
        "func-style": "off",
        "function-call-argument-newline": "off",
        "function-paren-newline": "off",
        "id-length": "off",
        "indent": ["error", 4, {
            "SwitchCase": 1
        }],
        "max-len": "off",
        "max-lines": "off",
        "max-lines-per-function": "off",
        "max-params": "off",
        "max-statements": "off",
        "multiline-comment-style": "off",
        "no-continue": "off",
        "no-extra-parens": ["error", "all", {
            "nestedBinaryExpressions": false
        }],
        "no-loop-func": "off",
        "no-magic-numbers": "off",
        "no-nested-ternary": "off",
        "no-ternary": "off",
        "padded-blocks": "off",
        "prefer-destructuring": "off",
        "quote-props": "off",
        "quotes": ["error", "single"],
        "space-before-function-paren": "off"
    }
}
